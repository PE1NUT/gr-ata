<?xml version="1.0"?>
<block>
  <name>ATA Stream</name>
  <key>ata_ata_stream</key>
  <category>[ATA]</category>
  <import>import ata</import>
  <make>ata.ata_stream($udpport, $ata_header, $payloadsize, $verbose)</make>
  <param>
    <name>UDP Port</name>
    <key>udpport</key>
    <value>50000</value>
    <type>int</type>
  </param>
  <param>
    <name>Header Format</name>
    <key>ata_header</key>
    <value>1</value>
    <type>int</type>
    <option>
      <name>New Header</name>
      <key>1</key>
    </option>
    <option>
      <name>Old Header</name>
      <key>0</key>
    </option>
  </param>
  <param>
    <name>Payload Size</name>
    <key>payloadsize</key>
    <value>4096</value>
    <type>int</type>
  </param>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
